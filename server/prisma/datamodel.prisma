type User {
  id: ID! @unique
  firstName: String!
  lastName: String!
  email: String!
  createdAt: DateTime!
  updatedAt: DateTime!
  children: [Child!]!
}

enum RateType {
  HOURLY
  FLAT
}

enum Gender {
  MALE, FEMALE
}

type Child {
  id: ID! @unique
  firstName: String!
  lastName: String!
  birthday: DateTime
  owner: User!
  rateAmount: Float!
  rateType: RateType!
  createdAt: DateTime!
  updatedAt: DateTime!
  dates: [Date!]!
  gender: Gender!
}

type Date {
  id: ID! @unique
  dateObjectId: String!
  owner: Child!
  month: Float!
  day: Float!
  year: Float!
  hours: Float!
  dayOfWeek: String
  notes: String
  paid: Float
  fixedRateChecked: Boolean
}
